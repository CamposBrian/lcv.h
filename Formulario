int gradarad ( float gra){
    float rad=0;
    rad=(gra*PI)/180;
    cout<<rad;
     
     return 0;
    }
    int radgrad ( float rad){
    	float grad=0;
    	grad=(rad*180)/PI;
    	cout<<grad;
    	
    	return 0;
    }
    
    int distp1p2 (float x1=0, float y1=0, float z1=0, float x2=0, float y2=0, float z2=0){
    	float p1p2=sqrt(((x2 - x1)*(x2 -x1))+((y2 -y1)*(y2 - y1))+((z2 - z1)*(z2 - z1)));
    	cout<<p1p2;
    	
    	return 0;
    }
    
    int rectapol(float x=0, float y=0){
    	float r=0;
    	float teta=0;
    	r=sqrt((x*x)+(y*y));
    	teta=atan(y/x);
    	cout<<"("<<r<<","<<teta<<")";
    	return 0;
    }
    
    int esfacil(float r, float teta, float phy ){
    	float x=0;
    	float y=0;
    	float z=0;
    	x=r*sin(phy)*cos(teta);
    	cout<<x<<","<<y<<","<<z<<endl;
    	return 0;
    }
    
    int upuntov(float u1, float u2, float u3, float v1, float v2, float v3){
    	float w;
    	w=(u1)*(v1)+(u2)*(v2)+(u3)*(v3);
    	cout<<w;
    	return 0;
    }
    
    int umasv(double u1 , double u2, double u3, double v1, double v2, double v3){
    	float w1, w2, w3;
    	w1= u1 + v1;
    	w2= u2 + v2;
    	w3= u3 + v3;
    	cout<<w1<<","<<w2<<","<<w3<<endl;
        return 0;
    }
    
