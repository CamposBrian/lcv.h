#include<iostream>
#include<cmath>
#define PI 3.14159
#define E 2.7182818

using namespace std;
float x=0;
float r=0;
float teta=0;
float y=0;
{
	x=r*cos(teta);
	y=r*sin(teta);
	
	cout<<x <<y;
	return 0;

